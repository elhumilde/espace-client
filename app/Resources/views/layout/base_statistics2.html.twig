                          'Les 7 derniers jours': [moment().subtract(6, 'days'), moment()],
                          'Les 30 derniers jours': [moment().subtract(29, 'days'), moment()],
                          'Mois dernier': [moment().subtract(1, 'month').startOf('month'), moment().subtract(1, 'month').endOf('month')]
                        };
                      }

                      options.locale = {
                        
                          format: 'DD/MM/YYYY',
